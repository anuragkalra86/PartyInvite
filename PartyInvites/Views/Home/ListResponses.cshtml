@model IEnumerable<PartyInvites.Models.GuestResponse>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ListResponses</title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Phone</th>
            </tr>
        </thead>
        <tbody>
            @foreach(PartyInvites.Models.GuestResponse guestResponse in Model)
            {
            <tr>
                <td>@guestResponse.Name</td>
                <td>@guestResponse.Email</td>
                <td>@guestResponse.Phone</td>
            </tr>
            }
        </tbody>
    </table>
</body>
</html>
